package Model;

import java.awt.*;

public enum CoresMapa
{
	Vazio (Color.white),
	VazioAtingido (new Color (177, 203, 255)),
	Couracado (Color.orange),
	CouracadoSelecionado (new Color (177, 203, 255)),
	Submarino (Color.blue),
	SubmarinoSelecionado (new Color (177, 203, 255)),
	Cruzador (Color.green),
	CruzadorSelecionado (new Color (177, 203, 255)),
	Hidroaviao (Color.cyan),
	HidroaviaoSelecionado (new Color (177, 203, 255)),
	Destroyer (Color.black),
	DestroyerSelecionado (new Color (177, 203, 255)),
	Destruida (Color.gray),
	Bloqueado (new Color (177, 203, 255));
		
	private final Color cor;

    private CoresMapa (Color cor)
    {
        this.cor = cor;
    }
}