package Model;

import java.awt.*;

public enum CoresArmas
{
	Vazio (Color.white),
	VazioAtingido (),
	Couracado (),
	CouracadoSelecionado (),
	Submarino (),
	SubmarinoSelecionado (),
	Cruzador (),
	CruzadorSelecionado (),
	Hidroaviao (),
	HidroaviaoSelecionado (),
	Destroyer (),
	DestroyerSelecionado (),
	Destruida ();
	
	private final Color cor;

    private CoresArmas (Color cor) {
        this.cor = cor;
    }
}