package Model;

public enum CoresMapa
{
	package Model;

	import java.awt.*;

	public enum CoresArmas
	{
		Vazio (Color.white),
		VazioAtingido (),
		Couracado (Color.orange),
		CouracadoSelecionado (),
		Submarino (Color.blue),
		SubmarinoSelecionado (),
		Cruzador (Color.green),
		CruzadorSelecionado (),
		Hidroaviao (Color.cyan),
		HidroaviaoSelecionado (),
		Destroyer (Color.black),
		DestroyerSelecionado (),
		Destruida ();
		
		private final Color cor;

	    private CoresMapa (Color cor) {
	        this.cor = cor;
	    }
	}
}